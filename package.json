{
  "name": "ilp-settlement-general",
  "version": "0.0.1",
  "description": "A general ILP settlement engine",
  "main": "build/index.js",
  "scripts": {
    "start": "ts-node src/launch.ts",
    "build": "tsc",
    "clean": "rm -rf build && rm -rf node_modules",
    "debug": "tsc && DEBUG=* node build/launch.js",
    "lint": "tslint --project .",
    "test": "mocha -r ts-node/register test/**/*.test.ts",
    "next": "next build src/frontend && next start src/frontend",
    "next:start": "next start src/frontend",
    "next:build": "next build src/frontend",
    "next:dev": "next dev src/frontend",
    "dock": "docker-compose build && docker-compose up",
    "dock:start": "docker-compose up",
    "dock:build": "docker-compose build",
    "stripe": "ts-node src/examples/stripe/stripe.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/barronwei/ilp-settlement-general.git"
  },
  "author": "Barron Wei",
  "license": "Apache-2.0",
  "dependencies": {
    "axios": "^0.19.0",
    "ioredis": "^4.11.2",
    "koa": "^2.7.0",
    "koa-bodyparser": "^4.2.1",
    "koa-router": "^7.4.0",
    "next": "^9.0.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/ioredis": "^4.0.13",
    "@types/koa": "^2.0.49",
    "@types/koa-bodyparser": "^4.3.0",
    "@types/koa-router": "^7.0.42",
    "@types/mocha": "^5.2.7",
    "@types/next": "^8.0.6",
    "@types/react": "^16.8.24",
    "@types/react-dom": "^16.8.5",
    "@types/sinon": "^7.0.13",
    "@types/stripe": "^6.31.10",
    "@types/uuid": "^3.4.5",
    "@zeit/next-typescript": "^1.1.1",
    "camelcase-keys": "^6.0.0",
    "chai": "^4.2.0",
    "ioredis-mock": "^4.12.1",
    "mocha": "^6.1.4",
    "mocha-typescript": "^1.1.17",
    "mockttp": "^0.16.1",
    "react-stripe-elements": "^4.0.0",
    "sinon": "^7.3.2",
    "source-map-support": "^0.5.12",
    "stripe": "^7.5.0",
    "ts-node": "^8.3.0",
    "tslint": "^5.18.0",
    "tslint-config-standard": "^8.0.1",
    "typescript": "^3.5.3"
  }
}
